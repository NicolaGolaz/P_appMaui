<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="P_AppMobileLecture.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:P_AppMobileLecture"
    Shell.FlyoutBehavior="Disabled"
    Title="P_AppMobileLecture">

    <TabBar>
        <Tab Title="Home" Icon="images.png">
           
            <ShellContent ContentTemplate="{DataTemplate local:HomePage}" Route="HomePage" />
        </Tab>
        <Tab Title="Profile">
            <ShellContent ContentTemplate="{DataTemplate local:Profile}" Route="Profile" />
        </Tab>
    </TabBar>

</Shell>
